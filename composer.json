{
    "name": "spryker-demo/merchant-review-widget",
    "type": "library",
    "description": "MerchantReviewWidget module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker-demo/merchant-review": "^1.0.0",
        "spryker-demo/merchant-review-search": "^1.0.0",
        "spryker-shop/shop-application": "^1.13.0",
        "spryker/application": "^3.32.0",
        "spryker/customer": "^7.51.0",
        "spryker/kernel": "^3.70.0",
        "spryker/symfony": "^3.12.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/router": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "spryker/router": "To use router plugins"
    }
}
